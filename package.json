{
  "name": "creepy-react",
  "description": "React/Redux based content site",
  "version": "0.0.1",
  "scripts": {
    "eslint": "node ./node_modules/eslint/bin/eslint.js --ext .js './app/**'",
    "eslint:fix": "node ./node_modules/eslint/bin/eslint.js --ext .js './app/**' --fix",
    "eslint:report": "node ./node_modules/eslint/bin/eslint.js --ext .js -f html -o ./reports/eslint.html ./app/**",
    "nodemon:dev": "cross-env NODE_ENV=development nodemon",
    "nodemon:dev:inspect": "cross-env NODE_ENV=development node --inspect node bin/creepy-web.js",
    "pm2:dev": "pm2 start bin/process.yml --env development",
    "pm2:prod": "pm2 start bin/process.yml --env production",
    "webpack": "webpack --progress --colors",
    "webpack:watch": "webpack --watch --progress --colors",
    "dev": "run-p nodemon:dev webpack:watch"
  },
  "nodemonConfig": {
    "exec": "node bin/creepy-web.js",
    "ignore": [
      "app/*",
      "bin/*",
      "build/*",
      "public/*"
    ],
    "delay": "2000"
  },
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cross-env": "^5.1.6",
    "ejs": "^2.5.9",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.12.0",
    "express": "^4.16.3",
    "express-device": "^0.4.2",
    "express-http-proxy": "^1.2.0",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "iconv-lite": "^0.4.21",
    "immutable": "^3.8.2",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.4.4",
    "react-recaptcha": "^2.3.7",
    "react-redux": "^5.0.7",
    "react-responsive": "^5.0.0",
    "react-router": "^4.2.0",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "socket.io": "^2.4.0",
    "superagent": "^4.0.0-beta.5",
    "swig-templates": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-syntax-export-default-from": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "delay": "x",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "x",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-rename": "x",
    "gulp-sass": "^4.0.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "regenerator-runtime": "^0.12.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT"
}
