{
  "name": "creepy-react",
  "description": "React/Redux based content site",
  "version": "0.0.1",
  "scripts": {
    "eslint": "node ./node_modules/eslint/bin/eslint.js --ext .js ./app/**",
    "eslint:report": "node ./node_modules/eslint/bin/eslint.js --ext .js -f html -o ./reports/eslint.html ./app/**",
    "dev": "cross-env NODE_ENV=development nodemon",
    "dev:inspect": "cross-env NODE_ENV=development node --inspect node bin/creepy-web.js",
    "pm2:dev": "pm2 start bin/process.yml --env development",
    "pm2:prod": "pm2 start bin/process.yml --env production",
    "gulp:dev": "gulp build-dev",
    "gulp:prod": "gulp build-prod",
    "gulp:watch": "gulp watch-styles",
    "webpack": "webpack --progress --colors"
  },
  "nodemonConfig": {
    "exec": "node bin/creepy-web.js",
    "ignore": [
      "app/*",
      "bin/*",
      "build/*",
      "public/*"
    ],
    "delay": "2000"
  },
  "dependencies": {
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "babel-plugin-transform-object-assign": "x",
    "babel-plugin-transform-runtime": "x",
    "babel-preset-stage-0": "x",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "cross-env": "^5.1.6",
    "ejs": "^2.5.9",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "express": "^4.16.3",
    "express-device": "^0.4.2",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "iconv-lite": "^0.4.21",
    "immutable": "^3.8.2",
    "moment": "^2.22.1",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.4.4",
    "react-recaptcha": "^2.3.7",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "socket.io": "^2.1.0",
    "superagent": "^3.8.3",
    "swig-templates": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "x",
    "babel-plugin-react-transform": "x",
    "babel-plugin-transform-object-rest-spread": "x",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.11",
    "delay": "x",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "x",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-rename": "x",
    "gulp-sass": "^4.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.0.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT"
}
